  <string name="tutorial_dialog_ok">ശരി</string>
  <string name="tutorial_01_title">3 ഘട്ടങ്ങളിലൂടെ ഡൌൺലോഡ് ചെയ്യാൻ ആരംഭിക്കുക</string>
